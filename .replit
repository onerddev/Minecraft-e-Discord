modules = ["python-3.11", "nodejs-20"]
[agent]
expertMode = true
integrations = ["python_gemini:1.0.0", "python_anthropic:1.0.0"]

[nix]
channel = "stable-25_05"
packages = ["ffmpeg", "iana-etc", "libsodium"]

[workflows]
runButton = "Project"

[[workflows.workflow]]
name = "Project"
mode = "parallel"
author = "agent"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "QG"

[[workflows.workflow]]
name = "QG"
author = "agent"

[workflows.workflow.metadata]
outputType = "console"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "python main.py"

[[ports]]
localPort = 3000
externalPort = 3001

[[ports]]
localPort = 5000
externalPort = 80

[deployment]
deploymentTarget = "vm"
run = ["python", "main.py"]

